datatype PROCNAMES = sender | receiver | t1
channel InvR,InvP : PROCNAMES
channel I_Setmessagt1,I_Setmessage2,I_Setmessage3,I_PrintMessage
RECEIVER = InvP.t1 -> I_PrintMessage -> RECEIVER
SENDER = I_Setmessagt1 -> InvR.t1 -> SENDER [] I_Setmessage2 -> InvR.t1 -> SENDER [] I_Setmessage3 -> InvR.t1 -> SENDER
T1 = InvP.sender -> InvR.receiver -> T1

senderreceiver = (SENDER ||| RECEIVER)[|{|InvR,InvP|}|](T1[[InvP.sender <- InvR.t1,InvR.receiver <- InvP.t1]])
assert senderreceiver :[deadlock free]
datatype PROCNAMES = client | server | t1
channel InvR,TerR,InvP,TerP : PROCNAMES
channel I_Process,I_Setmessage,I_Printmessage
SERVER = InvP.t1 -> I_Process -> TerP.t1 -> SERVER
CLIENT = I_Setmessage -> InvR.t1 -> TerR.t1 -> I_Printmessage -> CLIENT
T1 = InvP.client -> InvR.server -> TerR.server -> TerP.client -> T1

senderreceiver = (CLIENT ||| SERVER)[|{|TerP,InvR,TerR,InvP|}|](T1[[InvP.client <- InvR.t1,InvR.server <- InvP.t1,TerR.server <- TerP.t1,TerP.client <- TerR.t1]])
assert senderreceiver :[deadlock free]
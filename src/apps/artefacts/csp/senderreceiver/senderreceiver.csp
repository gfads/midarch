datatype PROCNAMES = s | r | t

channel InvR,InvP : PROCNAMES
channel I_Setmessage1,I_Setmessage2,I_Printmessage

R = InvP.t -> I_Printmessage -> R
S = I_Setmessage1 -> InvR.t -> S [] I_Setmessage2 -> InvR.t -> S
T = InvP.s -> InvR.r -> T

senderreceiver = (S ||| R)[|{|InvP,InvR|}|](T[[InvP.s <- InvR.t,InvR.r <- InvP.t]])

assert senderreceiver :[deadlock free]
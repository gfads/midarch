Configuration queueingserver :=
   Components
      engine   : Notificationengine
      invoker  : Notificationengineinvoker
      srh      : SRH   @QUEUEING_PORT

   Connectors
      t1 : Requestreply
      t2 : Requestreply

   Attachments
      srh,t1,invoker
      invoker,t2,engine

   Adaptability
      Evolutive
EndConf
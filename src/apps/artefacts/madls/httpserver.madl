Configuration httpserver :=
   Components
      namingproxy : Namingproxy
      httpinvoker : HttpInvokerM
      requestor   : RequestorM
      crh         : CRHHttp    @NAMING_PORT
      srh         : SRHHttp    @FIBONACCI_PORT

   Connectors
      t1 : Requestreply
      t2 : Requestreply
      t3 : Requestreply

   Attachments
      namingproxy,t1,requestor
      requestor,t2,crh
      srh,t3,httpinvoker

   Adaptability
      None
EndConf 
